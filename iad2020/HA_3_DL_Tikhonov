      "source": [
        "import gensim\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import string\n",
        "\n",
        "import nltk\n",
        "nltk.download('punkt')\n",
        "\n",
        "from nltk.tokenize import word_tokenize"
      ],
      "execution_count": 11,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "[nltk_data] Downloading package punkt to /root/nltk_data...\n",
            "[nltk_data]   Package punkt is already up-to-date!\n"
          ],
          "name": "stdout"
        }
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "i4o3LzBAPX4n"
      },
      "source": [
        "def process_text_w2v(text):\n",
        "    return [word for word in word_tokenize(text.lower()) if word not in string.punctuation]"
      ],
      "execution_count": 12,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "RT-dchfMQsxk"
      },
      "source": [
        "PATH_TO_TRAIN_DATA = 'hseds-texts-2020/train.csv'\n",
        "df1 = pd.read_csv(PATH_TO_TRAIN_DATA)\n",
        "\n",
        "df1['negative'] = df1['negative'].apply(process_text_w2v)\n",
        "df1['positive'] = df1['positive'].apply(process_text_w2v)"
      ],
      "execution_count": 13,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "6V38m2ZfRsdZ"
      },
      "source": [
        "from sklearn.model_selection import train_test_split\n",
        "df_train1, df_test1 = train_test_split(df1)\n",
        "\n",
        "df_train1['general'] = df_train1['positive'] + df_train1['negative']\n",
        "df_test1['general'] = df_test1['positive'] + df_test1['negative']\n",
        "\n",
        "X_train_w2v, X_test_w2v, y_train_w2v, y_test_w2v = df_train1['general'], df_test1['general'], df_train1['score'], df_test1['score']"
      ],
      "execution_count": 14,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "GH5kYSJlPVun"
      },
      "source": [
        "# Обучили векторное представление слов\n",
        "size = 500\n",
        "w2v = gensim.models.Word2Vec(list(X_train_w2v), size=size, window=5)"
      ],
      "execution_count": 15,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "jvjbwZrMYumP"
      },
      "source": [
        "# Пример\n",
        "\n",
        "#w2v['hotel']"
      ],
      "execution_count": 16,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "5Mvu_I_IWaW0"
      },
      "source": [
        "def document_vector_mean(doc):\n",
        "\n",
        "    doc = [word for word in doc if word in w2v.wv.vocab]\n",
        "\n",
        "    try:\n",
        "      return np.mean(w2v[doc], axis=0)\n",
        "\n",
        "    except:\n",
        "      return np.zeros(size)"
      ],
      "execution_count": 17,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "eF27zFc1UDmY"
      },
      "source": [
        "X_train_wv = list(X_train_w2v.apply(document_vector_mean))\n",
        "X_test_wv = list(X_test_w2v.apply(document_vector_mean))"
      ],
      "execution_count": 18,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "TdV1RF0jZ_WO",
        "outputId": "03ab058b-768e-44dd-e47f-79e8f7eca18b"
      },
      "source": [
        "from sklearn.linear_model import LinearRegression\n",
        "\n",
        "np.random.seed(10)\n",
        "linreg = LinearRegression()\n",
        "\n",
