      "source": [
        "def document_vector_ft(doc):\n",
        "\n",
        "    doc = [word for word in doc if word in fasttext.wv.vocab]\n",
        "\n",
        "    try:\n",
        "      return np.mean(fasttext[doc], axis=0)\n",
        "\n",
        "    except:\n",
        "      return np.zeros(size)"
      ],
      "execution_count": 30,
      "outputs": []
    },
