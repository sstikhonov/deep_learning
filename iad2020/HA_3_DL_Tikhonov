        "import numpy as np\n",
        "import string\n",
        "\n",
        "import nltk\n",
        "nltk.download('punkt')\n",
        "\n",
        "from nltk.tokenize import word_tokenize"
      ],
      "execution_count": 37,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "[nltk_data] Downloading package punkt to /root/nltk_data...\n",
            "[nltk_data]   Package punkt is already up-to-date!\n"
          ],
          "name": "stdout"
        }
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "ZrW8hqa4M676"
      },
      "source": [
        "import torch\n",
        "from torch import nn\n",
        "from torch.nn import functional as F"
      ],
      "execution_count": 38,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "cZtA1i2wkltI"
      },
      "source": [
        "def process_text(text):\n",
        "    return [word for word in word_tokenize(text.lower()) if word not in string.punctuation]"
      ],
      "execution_count": 39,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "g6s9hC8ikj2O"
      },
      "source": [
        "PATH_TO_TRAIN_DATA = 'hseds-texts-2020/train.csv'\n",
        "df = pd.read_csv(PATH_TO_TRAIN_DATA)\n",
        "\n",
        "df['negative'] = df['negative'].apply(process_text)\n",
        "df['positive'] = df['positive'].apply(process_text)"
      ],
      "execution_count": 40,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "TIf2nBOHM676"
      },
      "source": [
        "WORDS = set()\n",
        "for sent in list(df['positive']):\n",
        "    for w in sent:\n",
        "        WORDS.add(w)\n",
        "        \n",
        "for sent in list(df['negative']):\n",
        "    for w in sent:\n",
        "        WORDS.add(w)"
      ],
      "execution_count": 41,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "--NBi3ZhvYUi"
      },
      "source": [
        "df['general'] = df['negative'] + df['positive']"
      ],
      "execution_count": 42,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "cj-Holq2M676"
      },
      "source": [
        "int2word = dict(enumerate(tuple(WORDS)))\n",
        "word2int = {w: ii for ii, w in int2word.items()}"
      ],
      "execution_count": 43,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "iTC1RJn4qDaE"
      },
      "source": [
        "from sklearn.model_selection import train_test_split\n",
        "df_train, df_test = train_test_split(df, train_size=75000, test_size=25000)"
      ],
      "execution_count": 44,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "0IOpz8SYm8T3"
      },
      "source": [
        "# Удалим ненужные колонки\n",
        "df_train.drop(columns=['review_id', 'positive', 'negative'], inplace=True)\n",
        "df_test.drop(columns=['review_id', 'positive', 'negative'], inplace=True)"
      ],
      "execution_count": 45,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "THfHbjHUxiyU"
      },
      "source": [
        "# Поменяем местами колонки\n",
        "cols = df_train.columns.tolist()\n",
        "cols = cols[-1:] + cols[:-1]\n",
        "df_train = df_train[cols]\n",
        "\n",
        "cols_t = df_test.columns.tolist()\n",
        "cols_t = cols_t[-1:] + cols_t[:-1]\n",
        "df_test = df_test[cols_t]"
      ],
      "execution_count": 46,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "0C9_gR4edA3t"
      },
      "source": [
        "from torch.nn.utils.rnn import pad_sequence\n",
        "class ReviewsDataset(torch.utils.data.Dataset):\n",
        "\n",
        "    def __init__(self, X, y):\n",
        "        self.X = pad_sequence([torch.as_tensor([word2int[w] for w in seq][:50]) for seq in X], \n",
        "                           batch_first=True) # Паддим описания отеля\n",
        "        self.y = torch.as_tensor(np.array(y))\n",
        "        \n",
        "    def __len__(self):\n",
        "        return len(self.X)\n",
        "    \n",
        "    def __getitem__(self, idx):\n",
        "        return self.X[idx], self.y[idx]"
      ],
      "execution_count": 47,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "uiojyt4SThxW"
      },
      "source": [
        "X_train, X_test, y_train, y_test = df_train['general'], df_test['general'], df_train['score'], df_test['score']\n",
        "\n",
        "# Отдельно сделаем валидационный датасет, чтобы во время обучения прогонять не 25к элементов (долго), а всего 10к\n",
        "X_val, y_val = df_test['general'][:10000], df_test['score'][:10000]\n",
        "train_dataset = ReviewsDataset(X_train, y_train)\n",
        "val_dataset = ReviewsDataset(X_val, y_val)\n",
        "test_dataset = ReviewsDataset(X_test, y_test)\n",
        "\n",
        "from torch.utils.data import DataLoader\n",
        "\n",
        "batch_size = 1\n",
        "\n",
        "train_loader = torch.utils.data.DataLoader(train_dataset, batch_size=batch_size)\n",
        "val_loader = torch.utils.data.DataLoader(val_dataset, batch_size=batch_size)\n",
        "test_loader = torch.utils.data.DataLoader(test_dataset, batch_size=batch_size)"
      ],
      "execution_count": 48,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "_ba7bdmWVkWh"
      },
      "source": [
        "import torch.nn as nn\n",
        "# Код нейронной сети взять с Kaggle:\n",
        "# https://www.kaggle.com/gabrielloye/simple-lstm-using-pytorch\n",
        "class SentimentNet(nn.Module):\n",
        "    def __init__(self, vocab_size, output_size, embedding_dim, hidden_dim, n_layers, drop_prob=0.2):\n",
        "        super(SentimentNet, self).__init__()\n",
        "        self.output_size = output_size\n",
        "        self.n_layers = n_layers\n",
        "        self.hidden_dim = hidden_dim\n",
        "        \n",
        "        self.embedding = nn.Embedding(vocab_size, embedding_dim)\n",
        "        self.lstm = nn.LSTM(embedding_dim, hidden_dim, n_layers, dropout=drop_prob, batch_first=True)\n",
        "        self.dropout = nn.Dropout(0.2)\n",
        "        self.fc1 = nn.Linear(hidden_dim, 1)\n",
        "    \n",
        "        \n",
        "    def forward(self, x, hidden):\n",
        "        batch_size = x.size(0)\n",
        "        x = x.long()\n",
        "        embeds = self.embedding(x)\n",
        "        lstm_out, hidden = self.lstm(embeds, hidden)\n",
        "        lstm_out = lstm_out.contiguous().view(-1, self.hidden_dim)\n",
        "        \n",
        "        out = self.dropout(lstm_out)\n",
        "        out = self.fc1(out)\n",
        "        \n",
        "        out = out.view(batch_size, -1)\n",
        "        out = out[:,-1]\n",
        "        return out, hidden\n",
        "    \n",
        "    def init_hidden(self, batch_size):\n",
        "        weight = next(self.parameters()).data\n",
        "        hidden = (weight.new(self.n_layers, batch_size, self.hidden_dim).zero_().to(device),\n",
        "                      weight.new(self.n_layers, batch_size, self.hidden_dim).zero_().to(device))\n",
        "        return hidden"
      ],
      "execution_count": 49,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "x2GDUxRbVxMG",
        "outputId": "55035a85-6b9e-4d28-8ddb-40cf5f4a0ad3"
      },
      "source": [
        "vocab_size = len(word2int) + 1\n",
        "output_size = 1\n",
        "embedding_dim = 450\n",
        "hidden_dim = 50\n",
        "n_layers = 2\n",
        "device = torch.device(\"cuda\")\n",
        "criterion = nn.L1Loss() #MAE\n",
        "model = SentimentNet(vocab_size, output_size, embedding_dim, hidden_dim, n_layers)\n",
        "model.to(device)\n",
        "optimizer = torch.optim.Adagrad(model.parameters(), lr=0.009)\n",
        "\n",
        "\n",
        "print(model)"
      ],
      "execution_count": 50,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
